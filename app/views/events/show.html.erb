<h2><%= @event.title %>
</h2>
<p><%= @event.description %></p>
<p><%= @date%></p>
<p><%= @time%></p>
<br>
<h3>Attendees:</h3>
<% @event.attendees.all.each do |attendee|%>
    <%= attendee.email%>
    <br>
    
<% end %>
<%= link_to "Attend?", attend_event_path(@event.id)%>
<% if flash[:notice] %>
  <div class="notice"><%= flash[:notice] %></div>
<% end %>

